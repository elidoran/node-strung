{
  "name": "strung",
  "version": "1.2.0",
  "description": "Acts as a stream to send a string, gather a string, or both.",
  "main": "lib/index.js",
  "files": [
    "lib/index.js"
  ],
  "keywords": [
    "string to stream",
    "string stream",
    "stream to string",
    "stream string",
    "stream from string",
    "string from stream",
    "convert",
    "string",
    "stream",
    "source",
    "sink",
    "pipe",
    "piping",
    "readable",
    "writable",
    "duplex",
    "io",
    "input",
    "output",
    "memory"
  ],
  "author": "Eli Doran <eli+npm@elidoran.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --no-eslintrc -c build/eslint/eslint.js --cache --cache-location build/eslint/cache lib/index.js",
    "test": "mocha --R spec --bail --check-leaks test/lib/*.js",
    "test-older": "nave use 10 npm test",
    "test-stable": "nave use stable npm test",
    "test-latest": "nave use latest npm test",
    "tests": "npm run test-older && npm run test-stable && npm run test-latest",
    "cover": "nyc --silent --no-clean npm run test",
    "cover-older": "nave use 10 npm run cover",
    "cover-stable": "nave use stable npm run cover",
    "cover-latest": "nave use latest npm run cover",
    "covers": "npm run cover-older && npm run cover-stable && npm run cover-latest",
    "coverage": "npm run covers && nyc report --reporter=lcov --reporter=text-summary",
    "travis": "npm run covers && nyc report --reporter=text-lcov | coveralls",
    "browser": "npm run cover && nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elidoran/node-strung.git"
  },
  "bugs": "http://github.com/elidoran/node-strung/issues",
  "devDependencies": {
    "coffee-script": "^1.12.7",
    "coveralls": "^3.1.0",
    "eslint": "^7.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nave": "^3.2.1",
    "nyc": "^15.0.1",
    "through2": "^3.0.1"
  },
  "dependencies": {},
  "engines": {
    "node": ">=6"
  }
}
